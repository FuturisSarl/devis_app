<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Composant">
    <ImageView
            android:id="@+id/imgMateriel"
            android:contentDescription="@string/app_name"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="24dp"
            app:srcCompat="@drawable/materiel" android:elevation="5dp"
            android:color="#42000000"
            android:radius="5dp" style="@style/AppTheme.ImgComposantPrincipal"
            app:layout_constraintEnd_toStartOf="@+id/imgFondation" app:layout_constraintHorizontal_bias="0.5"/>
    <ImageView
            android:id="@+id/imgElevation"
            android:contentDescription="@string/app_name"
            app:layout_constraintStart_toStartOf="@+id/imgMateriel"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="@+id/imgMateriel" app:layout_constraintHorizontal_bias="1.0"
            app:srcCompat="@drawable/elevation" style="@style/AppTheme.ImgComposantPrincipal"
            android:layout_marginTop="@dimen/img_composant_top_constraint"/>
    <ImageView
            android:id="@+id/imgElectricite"
            android:contentDescription="@string/app_name"
            app:layout_constraintStart_toStartOf="@+id/imgToiture"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="@+id/imgToiture" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="@dimen/img_composant_top_constraint" app:srcCompat="@drawable/electricite"
            style="@style/AppTheme.ImgComposantPrincipal"/>
    <ImageView
            android:id="@+id/imgFinition"
            android:contentDescription="@string/app_name"
            app:layout_constraintStart_toStartOf="@+id/imgElevation"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="@+id/imgElevation" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="@dimen/img_composant_top_constraint" app:srcCompat="@drawable/finition"
            style="@style/AppTheme.ImgComposantPrincipal"/>
    <ImageView
            android:id="@+id/imgMenuiserie"
            android:contentDescription="@string/app_name"
            app:layout_constraintStart_toStartOf="@+id/imgFinition"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/imgFinition" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="@dimen/img_composant_top_constraint" app:srcCompat="@drawable/menuiserie"
            style="@style/AppTheme.ImgComposantPrincipal"/>
    <ImageView
            android:id="@+id/imgPlomberie"
            android:contentDescription="@string/app_name"
            app:layout_constraintStart_toStartOf="@+id/imgElectricite"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/imgElectricite" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="@dimen/img_composant_top_constraint" app:srcCompat="@drawable/plomberie"
            style="@style/AppTheme.ImgComposantPrincipal"/>
    <ImageView
            android:id="@+id/imgFondation"
            android:contentDescription="@string/app_name"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="24dp"
            app:srcCompat="@drawable/fondation" style="@style/AppTheme.ImgComposantPrincipal"
            app:layout_constraintStart_toEndOf="@+id/imgMateriel" app:layout_constraintHorizontal_bias="0.5"/>
    <ImageView
            android:id="@+id/imgToiture"
            android:contentDescription="@string/app_name"
            app:layout_constraintEnd_toEndOf="@+id/imgFondation"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/imgFondation"
            android:layout_marginTop="@dimen/img_composant_top_constraint"
            app:layout_constraintHorizontal_bias="0.0" app:srcCompat="@drawable/toiture"
            style="@style/AppTheme.ImgComposantPrincipal"/>
    <TextView
            android:text="@string/materiels"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="@+id/imgMateriel"
            app:layout_constraintStart_toStartOf="@+id/imgMateriel"
            app:layout_constraintTop_toBottomOf="@+id/imgMateriel" android:layout_marginTop="8dp"/>
    <TextView
            android:text="@string/elevation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            app:layout_constraintEnd_toEndOf="@+id/imgElevation"
            app:layout_constraintStart_toStartOf="@+id/imgElevation" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imgElevation"/>
    <TextView
            android:text="@string/finition"
            android:layout_width="56dp"
            android:layout_height="15dp" android:id="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/imgFinition"
            app:layout_constraintStart_toStartOf="@+id/imgFinition"
            app:layout_constraintTop_toBottomOf="@+id/imgFinition" android:layout_marginTop="8dp"/>
    <TextView
            android:text="@string/menuiserie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="@+id/imgMenuiserie"
            app:layout_constraintStart_toStartOf="@+id/imgMenuiserie"
            app:layout_constraintTop_toBottomOf="@+id/imgMenuiserie" android:layout_marginTop="8dp"/>
    <TextView
            android:text="@string/toiture"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/imgToiture" app:layout_constraintEnd_toEndOf="@+id/imgToiture"
            app:layout_constraintTop_toBottomOf="@+id/imgToiture" android:layout_marginTop="8dp"/>
    <TextView
            android:text="@string/fondation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            app:layout_constraintEnd_toEndOf="@+id/imgFondation"
            app:layout_constraintStart_toStartOf="@+id/imgFondation"
            app:layout_constraintTop_toBottomOf="@+id/imgFondation" android:layout_marginTop="8dp"/>
    <TextView
            android:text="@string/electricite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/imgElectricite"
            app:layout_constraintStart_toStartOf="@+id/imgElectricite"
            app:layout_constraintTop_toBottomOf="@+id/imgElectricite" android:layout_marginTop="8dp"/>
    <TextView
            android:text="@string/plomberie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="@+id/imgPlomberie"
            app:layout_constraintStart_toStartOf="@+id/imgPlomberie"
            app:layout_constraintTop_toBottomOf="@+id/imgPlomberie" android:layout_marginTop="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>